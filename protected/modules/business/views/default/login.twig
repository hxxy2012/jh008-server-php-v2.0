        <p class="remember_me">
          <label>
            <input type="checkbox" name="remember_me" id="remember_me">
              记住我
          </label>
        </p>
        <p class="submit">
          <a href="javascript:;" id="login" class="login-btn">登录</a>
        </p>
      </form>
    </div>
  </div>
  
