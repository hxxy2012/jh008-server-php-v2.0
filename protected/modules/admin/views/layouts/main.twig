<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html;charset=UTF-8">
        <title>零创科技后台系统</title>
        <link media="all" href="{{ this.module.assetsUrl }}/sea-modules/alice/one/1.2.2/one.css" rel="stylesheet">
        <link rel="stylesheet" href="{{ this.module.assetsUrl }}/sea-modules/arale/dialog/1.3.1/dialog.css" />
        <link rel="stylesheet" href="{{ this.module.assetsUrl }}/static/css/compontent.css" />
        <link rel="stylesheet" href="{{ this.module.assetsUrl }}/static/css/main.css" />
        {% block stylesheet %}{% endblock %}
    </head>
    <body>
        <div class="header">
            <div class="header-wrap">
                <div class="header-topbar">
                    <a class="topbar-item" id="logout" href="javascript:;">退出登录</a>
                </div>
            </div>
        </div>

        <div class="container">
            <div class="left-side">
                <div class="ls-wrap">
                   <!--  <div class="ui-box mt50">
                       <div class="ui-box-head">
                           <h3 class="ui-box-head-title">用户管理</h3>
                           <i class="rotate slide-up"></i>
                       </div>
                       <div class="ui-box-container">
                           <ul class="ui-list ui-list-nosquare  ui-list-graylink">
                               <li class="ui-list-item"><a href="#">所有用户列表</a></li>
                               <li class="ui-list-item"><a href="#">流氓用户检测</a></li>
                               <li class="ui-list-item"><a href="#">商户列表</a></li>
                           </ul>
                       </div>
                   </div> -->
                    <div class="ui-box mt50">
                        <div class="ui-box-head">
                            <h3 class="ui-box-head-title">基础数据查询</h3>
                            <i class="rotate slide-up"></i>
                        </div>
                        <div class="ui-box-container">
                            <ul class="ui-list ui-list-nosquare  ui-list-graylink">
                                <li class="ui-list-item">
                                    <a class="{% if this.getAction().getId()=='usercount' %}sel{% endif %}" href="/admin/default/usercount">基础数据统计</a>
                                </li>
                            </ul>
                        </div>
                    </div>                   
                    <div class="ui-box">
                        <div class="ui-box-head">
                            <h3 class="ui-box-head-title">用户管理</h3>
                            <i class="rotate slide-up"></i>
                        </div>
                        <div class="ui-box-container">
                            <ul class="ui-list ui-list-nosquare  ui-list-graylink">
                                <li class="ui-list-item">
                                    <a class="{% if this.getAction().getId()=='users'  %}sel{% endif %}" href="/admin/default/users">用户列表</a>
                                </li>
                                <li class="ui-list-item">
                                    <a class="{% if this.getAction().getId()=='business'  %}sel{% endif %}" href="/admin/default/business">商户列表</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                    <div class="ui-box">
                        <div class="ui-box-head">
                            <h3 class="ui-box-head-title">应用管理</h3>
                            <i class="rotate slide-up"></i>
                        </div>
                        <div class="ui-box-container">
                            <ul class="ui-list ui-list-nosquare  ui-list-graylink">
                                <li class="ui-list-item">
                                    <a class="{% if this.getAction().getId()=='acts'  %}sel{% endif %}" href="/admin/default/acts">活动列表</a>
                                </li>
                                <li class="ui-list-item">
                                    <a class="{% if this.getAction().getId()=='recommends'  %}sel{% endif %}" href="/admin/default/recommends">推荐信息列表</a>
                                </li>                               
                                <li class="ui-list-item">
                                    <a class="{% if this.getAction().getId()=='tags'  %}sel{% endif %}" href="/admin/default/tags">标签管理</a>
                                </li>  
                                <li class="ui-list-item">
                                    <a class="{% if this.getAction().getId()=='checkins'  %}sel{% endif %}" href="/admin/default/checkins">签到模块</a>
                                </li>  
                                <li class="ui-list-item">
                                    <a class="{% if this.getAction().getId()=='msgs'  %}sel{% endif %}" href="/admin/default/msgs">消息管理</a>
                                </li>   
                                <li class="ui-list-item">
                                    <a class="{% if this.getAction().getId()=='msgTypes'  %}sel{% endif %}" href="/admin/default/msgTypes">消息类型管理</a>
                                </li>                                  
                                <li class="ui-list-item">
                                    <a class="{% if this.getAction().getId()=='pushs'  %}sel{% endif %}" href="/admin/default/pushs">push管理</a>
                                </li>   
                                <li class="ui-list-item">
                                    <a class="{% if this.getAction().getId()=='pushTypes'  %}sel{% endif %}" href="/admin/default/pushTypes">push类型管理</a>
                                </li>                                  
                                <li class="ui-list-item">
                                    <a class="{% if this.getAction().getId()=='apps'  %}sel{% endif %}" href="/admin/default/apps">版本管理</a>
                                </li>    
                            </ul>
                        </div>
                    </div>
                    <div class="ui-box">
                        <div class="ui-box-head">
                            <h3 class="ui-box-head-title">管理员管理</h3>
                            <i class="rotate slide-up"></i>
                        </div>
                        <div class="ui-box-container">
                            <ul class="ui-list ui-list-nosquare  ui-list-graylink">
                                <li class="ui-list-item">
                                    <a class="{% if this.getAction().getId()=='adminviewinfo'  %}sel{% endif %}" href="/admin/default/adminviewinfo">个人资料</a>
                                </li>
                                <li class="ui-list-item">
                                    <a class="{% if this.getAction().getId()=='admins'  %}sel{% endif %}" href="/admin/default/admins">管理员列表</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                    <div class="ui-box">
                        <div class="ui-box-head">
                            <h3 class="ui-box-head-title">抽奖模块</h3>
                            <i class="rotate slide-up"></i>
                        </div>
                        <div class="ui-box-container">
                            <ul class="ui-list ui-list-nosquare  ui-list-graylink">
                                <li class="ui-list-item">
                                    <a class="{% if this.getAction().getId()=='prize'  %}sel{% endif %}" href="/admin/default/prize">抽奖</a>
                                </li>
                            </ul>
                        </div>
                    </div>                   
                </div>
            </div>
            <div class="right-side">
                <div class="right-side-container rsc">
                    {% block content %}{% endblock %}
                </div>
            </div>
        </div>
        
        

        {% block  template %}{% endblock %}

        <script>
            var basePath = "{{ this.module.assetsUrl }}";
            var imgUrlPre = "{{ App.params.imgUpload.downUrlPre}}";
        </script>
        <script type="text/javascript"  src="{{ this.module.assetsUrl }}/sea-modules/seajs/seajs/2.2.1/sea.js"></script>
        <script type="text/javascript"  src="{{ this.module.assetsUrl }}/static/js/third/artTemplate.js"></script>
        <script type="text/javascript"  src="{{ this.module.assetsUrl }}/static/js/third/jquery-1.8.2.js"></script>
        <script type="text/javascript"  src="{{ this.module.assetsUrl }}/static/js/main/config.js"></script>


        {% block javascript %}{% endblock %}

    </body>
</html>
